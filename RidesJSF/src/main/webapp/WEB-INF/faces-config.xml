<?xml version="1.0" encoding="UTF-8"?>

<faces-config
 xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
 version="2.0">
	<managed-bean>
		<managed-bean-name>create</managed-bean-name>
		<managed-bean-class>eredua.bean.CreateBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>query</managed-bean-name>
		<managed-bean-class>eredua.bean.QueryBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>login</managed-bean-name>
		<managed-bean-class>eredua.bean.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>profile</managed-bean-name>
		<managed-bean-class>eredua.bean.ProfileBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>showDriverRides</managed-bean-name>
		<managed-bean-class>eredua.bean.ShowDriverRidesBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>showNext</managed-bean-name>
		<managed-bean-class>eredua.bean.ShowNextBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>Menu.xhtml</display-name>
		<from-view-id>/Menu.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>create</from-outcome>
			<to-view-id>/CreateRide.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Menu.xhtml</display-name>
		<from-view-id>/Menu.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>query</from-outcome>
			<to-view-id>/QueryRides.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateRide.xhtml</display-name>
		<from-view-id>/CreateRide.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>return</from-outcome>
			<to-view-id>/Menu.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>QueryRides.xhtml</display-name>
		<from-view-id>/QueryRides.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>return</from-outcome>
			<to-view-id>/Menu.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Login.xhtml</display-name>
		<from-view-id>/Login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>ok</from-outcome>
			<to-view-id>/Menu.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Menu.xhtml</display-name>
		<from-view-id>/Menu.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>bye</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Login.xhtml</display-name>
		<from-view-id>/Login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>newuser</from-outcome>
			<to-view-id>/Register.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Register.xhtml</display-name>
		<from-view-id>/Register.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>QueryRides.xhtml</display-name>
		<from-view-id>/QueryRides.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>bye</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateRide.xhtml</display-name>
		<from-view-id>/CreateRide.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>bye</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>QueryRides.xhtml</display-name>
		<from-view-id>/QueryRides.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>createRide</from-outcome>
			<to-view-id>/CreateRide.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateRide.xhtml</display-name>
		<from-view-id>/CreateRide.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>queryRides</from-outcome>
			<to-view-id>/QueryRides.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Menu.xhtml</display-name>
		<from-view-id>/Menu.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>profile</from-outcome>
			<to-view-id>/Profile.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Profile.xhtml</display-name>
		<from-view-id>/Profile.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>createRide</from-outcome>
			<to-view-id>/CreateRide.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>QueryRides.xhtml</display-name>
		<from-view-id>/QueryRides.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>profile</from-outcome>
			<to-view-id>/Profile.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Profile.xhtml</display-name>
		<from-view-id>/Profile.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>return</from-outcome>
			<to-view-id>/Menu.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Profile.xhtml</display-name>
		<from-view-id>/Profile.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>queryRides</from-outcome>
			<to-view-id>/QueryRides.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Profile.xhtml</display-name>
		<from-view-id>/Profile.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>bye</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateRide.xhtml</display-name>
		<from-view-id>/CreateRide.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>profile</from-outcome>
			<to-view-id>/Profile.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>ShowDriverRides.xhtml</display-name>
		<from-view-id>/ShowDriverRides.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>return</from-outcome>
			<to-view-id>/Menu.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>ShowDriverRides.xhtml</display-name>
		<from-view-id>/ShowDriverRides.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>bye</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Menu.xhtml</display-name>
		<from-view-id>/Menu.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>show</from-outcome>
			<to-view-id>/ShowDriverRides.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>ShowDriverRides.xhtml</display-name>
		<from-view-id>/ShowDriverRides.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>show</from-outcome>
			<to-view-id>/ShowNext.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>ShowNext.xhtml</display-name>
		<from-view-id>/ShowNext.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>bye</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>ShowNext.xhtml</display-name>
		<from-view-id>/ShowNext.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/ShowDriverRides.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>ShowNext.xhtml</display-name>
		<from-view-id>/ShowNext.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>return</from-outcome>
			<to-view-id>/Menu.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
